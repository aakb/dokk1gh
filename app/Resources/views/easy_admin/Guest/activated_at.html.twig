{% if value %}
    {% include 'easy_admin/datetime.html.twig' %}
{% else %}
    {% set action_href = path('easyadmin', app.request.query.all|merge({ action: 'sendApp', id: item.id })) %}
    <form method="post" action="{{ action_href }}">
        <button type="submit" class="btn btn-default">{{ 'Send app'|trans }}</button>
    </form>
{% endif %}
